{
    "name": "pramana/php-telegram-reminder",
    "description": "A Telegram Reminder based on PHP 7",
    "keywords": ["telegram", "php", "reminder"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.8",
        "slim/twig-view": "^2.2",
        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4",
        "illuminate/database": "5.6.11",
        "dragonmantank/cron-expression": "^2.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
